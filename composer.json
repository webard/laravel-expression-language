{
    "name": "webard/laravel-expression-language",
    "description": "Package Description",
    "keywords": [
        "webard",
        "laravel",
        "symfony",
        "eval",
        "expression"
    ],
    "homepage": "https://github.com/webard/laravel-expression-language",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Webard",
            "email": "code@webard.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^11.1",
        "illuminate/contracts": "^11.1",
        "illuminate/support": "^10.0|^11.0",
        "symfony/expression-language": "^7.0"
    },
    "require-dev": {
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.15",
        "orchestra/testbench": "^9.0.3",
        "pestphp/pest": "^2.34",
        "phpunit/phpunit": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Webard\\LaravelExpressionLanguage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webard\\LaravelExpressionLanguage\\Tests\\": "tests",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve"
        ],
        "lint": [
            "@php vendor/bin/pint",
            "@php vendor/bin/phpstan analyse"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webard\\LaravelExpressionLanguage\\ServiceProvider"
            ],
            "aliases": {
                "ExpressionLanguage": "Webard\\LaravelExpressionLanguage\\Facade\\ExpressionLanguage"
            }
        }
    }
}